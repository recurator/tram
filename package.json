{
  "name": "@openclaw/tram",
  "version": "0.2.0",
  "description": "TRAM - Tiered Reversible Associative Memory for OpenClaw",
  "type": "module",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "openclaw": {
    "extensions": [
      "./dist/index.js"
    ],
    "hooks": [
      "./dist/hooks/auto-recall",
      "./dist/hooks/auto-capture"
    ]
  },
  "scripts": {
    "build": "tsc && npm run build:hooks",
    "build:hooks": "cp hooks/auto-recall/HOOK.md dist/hooks/auto-recall/ && cp hooks/auto-capture/HOOK.md dist/hooks/auto-capture/",
    "typecheck": "tsc --noEmit",
    "test": "vitest run",
    "test:watch": "vitest"
  },
  "keywords": [
    "openclaw",
    "memory",
    "tiered",
    "sqlite",
    "embeddings"
  ],
  "license": "MIT",
  "devDependencies": {
    "@types/better-sqlite3": "^7.6.13",
    "@types/node": "^25.1.0",
    "typescript": "^5.3.0",
    "vitest": "^3.0.0"
  },
  "dependencies": {
    "@sinclair/typebox": "^0.34.48",
    "@xenova/transformers": "^2.17.2",
    "better-sqlite3": "^12.6.2",
    "zod": "^4.3.6"
  },
  "peerDependencies": {
    "openclaw": ">=2026.0.0"
  },
  "peerDependenciesMeta": {
    "openclaw": {
      "optional": true
    }
  }
}
